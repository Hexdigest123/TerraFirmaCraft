      }
    },
	{
      "when": {
        "part": "lower",
        "age": "2"
      },
      "apply": {
        "model": "tfc:block/plant/switchgrass_short_dynamic"
      }
    },
	{
      "when": {
        "part": "lower",
        "age": "3"
      },
      "apply": {
        "model": "tfc:block/plant/switchgrass_dynamic"
      }
    }
  ]
}