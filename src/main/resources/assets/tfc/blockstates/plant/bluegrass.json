{
  "multipart": [
    {
      "when": {
        "age": "0"
      },
      "apply": {
        "model": "tfc:block/plant/bluegrass_shortest_dynamic"
      }
    },
    {
      "when": {
        "age": "1"
      },
      "apply": {
        "model": "tfc:block/plant/bluegrass_shorter_dynamic"
      }
    },
    {
      "when": {
        "age": "2"
      },
      "apply": {
        "model": "tfc:block/plant/bluegrass_short_dynamic"
      }
    },
    {
      "when": {
        "age": "3"
      },
      "apply": {
        "model": "tfc:block/plant/bluegrass_dynamic"
      }
    }
  ]
}